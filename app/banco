CREATE DATABASE quiz;
USE quiz;
CREATE TABLE personalidade(
	id           INT AUTO_INCREMENT,
	nome         VARCHAR(100) NOT NULL,
	email        VARCHAR(100) NOT NULL,
	porcentagemR INT,
	porcentagemI INT,
	porcentagemA INT,
	porcentagemS INT,
	porcentagemE INT,
	porcentagemC INT,

	PRIMARY KEY(id)
);
